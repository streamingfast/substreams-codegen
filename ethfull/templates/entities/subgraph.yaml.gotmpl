specVersion: 0.0.6
description: {{ .Name }} substreams based subgraph
repository:  # fill in with git remote url
schema:
  file: ./schema.graphql

dataSources:
  - kind: substreams
    name: {{ .Name }}
    network: {{ .ChainName }}
    source:
      package:
        moduleName: graph_out
        file: {{ .SubgraphProjectName }}-v0.1.0.spkg
    mapping:
      kind: substreams/graph-entities
      apiVersion: 0.0.5
